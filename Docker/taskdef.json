{
   "containerDefinitions": [ 
      { 
         "essential": true,
         "image": "100411939642.dkr.ecr.us-east-1.amazonaws.com/idolnginx:latest",
         "name": "we",
         "portMappings": [ 
            { 
               "containerPort": 80,
               "hostPort": 80,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::100411939642:role/ecsTaskExecutionRole",
   "family": "tas1",
   "memory": "512",
   "networkMode": "awsvpc",
   "requiresCompatibilities": [ 
       "FARGATE" 
    ]
}
